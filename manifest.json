{
  "manifestVersion": 5,
  "id": "com.pressdrop.generative-bleed",
  "name": "PressDrop Generative Bleed",
  "version": "1.0.0",
  "host": {
    "app": "PS",
    "minVersion": "24.0.0"
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "bleedPanel",
      "label": "Generative Bleed",
      "minimumSize": {
        "width": 320,
        "height": 420
      },
      "maximumSize": {
        "width": 600,
        "height": 800
      },
      "preferredDockedSize": {
        "width": 360,
        "height": 520
      },
      "preferredFloatingSize": {
        "width": 420,
        "height": 580
      },
      "mainPath": "index.html"
    }
  ]
}
